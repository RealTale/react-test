{
  "name": "react-mobx-react-router-boilerplate",
  "version": "1.0.17",
  "description": "React, MobX, React-Router, MobX-Router and Webpack (heroku-ready) boilerplate with async routes.",
  "main": "webpack.config.js",
  "engines": {
    "node": ">=8.x",
    "npm": ">=5.x",
    "yarn": ">=0.24.x"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=production rimraf ./dist && webpack --config webpack.config.production.js",
    "postinstall": "webpack --config ./webpack.config.production.js --display-error-details",
    "preview": "cross-env NODE_ENV=preview http-server ./dist -p 1234 -c-1",
    "test": "cross-env NODE_ENV=test echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdevero/react-mobx-react-router-boilerplate.git"
  },
  "keywords": [
    "app",
    "ES6",
    "ES7",
    "heroku",
    "mobx",
    "mobx-router",
    "react",
    "react-router",
    "reactjs",
    "web app",
    "Webpack"
  ],
  "author": "Alex Devero <deveroalex@gmail.com> (http://github.com/alexdevero)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexdevero/react-mobx-react-router-boilerplate/issues"
  },
  "homepage": "",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.0.0",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.10",
    "react-hot-loader": "3.1.3",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "css-declaration-sorter": "^3.0.1",
    "css-mqpacker": "^6.0.2",
    "file-loader": "^1.1.6",
    "http-server": "^0.11.1",
    "lazy-route": "^1.0.7",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-react-devtools": "^4.2.15",
    "mobx-react-router": "^4.0.1",
    "package": "^1.0.1",
    "postcss-cssnext": "^3.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "rfx-core": "^1.5.3",
    "whatwg-fetch": "^2.0.3"
  }
}
